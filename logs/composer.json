{
    "name": "quids/logs",
    "description": "Laravel logging package for Quids Logs SaaS platform",
    "keywords": [
        "laravel",
        "logging",
        "monitoring",
        "saas",
        "quids"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Spektor",
            "email": "robert@quids.io"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0",
        "monolog/monolog": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Quids\\Logs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Quids\\Logs\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Quids\\Logs\\QuidsLogsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}